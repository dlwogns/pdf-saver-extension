{
    "manifest_version": 3,
    "name": "Page to PDF",
    "version": "1.0",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["libs/html2pdf.bundle.min.js", "injected.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }